<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.shell.popr.prporel.controller.View1" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					    <sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true" layout="ResponsiveGridLayout" id="form0">
					        <sap.ui.layout.form:content>
					            <Label text="PO Number" id="label0"/>
					            <Input width="100%" id="POInput"/>
					            <Button xmlns="sap.m" text="GO" press="handleGoButtonPress" id="button0" width="100px"/></sap.ui.layout.form:content>
					    </sap.ui.layout.form:SimpleForm>
					    <ObjectHeader xmlns="sap.m" title="CreatedBy" number="{oFrontend>/CreatedBy}" id="header0" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" cd:w5g.dt.context="/PurchaseOrderSet">
					        <statuses>
					            <ObjectStatus text="Success" state="Success" id="status0"/>
					            
					        </statuses>
					        <attributes>
					            <ObjectAttribute text="10 units" id="attribute0"/>
					            
					        </attributes>
					        </ObjectHeader>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>